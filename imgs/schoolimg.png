</span><span><span class="hljs-selector-class">.navbar</span></span><span> </span><span><span class="hljs-selector-tag">ul</span></span><span> </span><span><span class="hljs-selector-tag">li</span></span><span> </span><span><span class="hljs-selector-tag">a</span></span><span><span class="hljs-selector-pseudo">:hover</span></span><span> {
